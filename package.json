{
  "name": "vue-example",
  "version": "1.0.0",
  "description": "Example vue with backend.",
  "main": "index.js",
  "scripts": {
    "dev": "run-p webpack-w server-w",
    "webpack-w": "webpack-dev-server --config build/public/webpack.dev.js --color --progress",
    "server-w": "nodemon --config nodemon.json",
    "build": "run-p webpack-build server-build",
    "webpack-build": "rimraf dist/public && webpack --config build/public/webpack.prod.js",
    "server-build": "rimraf dist/server && webpack --config build/server/webpack.prod.js",
    "clean": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mdi/font": "^4.6.95",
    "@types/browser-sync": "^2.26.1",
    "@types/express": "^4.17.1",
    "@types/vue": "^2.0.0",
    "@types/webpack": "^4.41.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fibers": "^4.0.2",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "postcss-cssnext": "^3.1.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.6.4",
    "vue-class-component": "^7.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "vue-property-decorator": "^8.3.0",
    "vuetify": "^2.1.11",
    "vuex": "^3.1.2"
  }
}
